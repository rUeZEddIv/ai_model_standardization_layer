# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ai_content_db?schema=public"

# Application
NODE_ENV=development
API_PORT=3000
API_PREFIX=api/v1

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this
JWT_EXPIRATION=7d

# Webhook
WEBHOOK_SECRET=your-webhook-verification-secret

# Providers - KIE.AI
KIE_API_BASE_URL=https://api.kie.ai
KIE_API_KEYS=key1,key2,key3

# Providers - GeminiGen.AI
GEMINIGEN_API_BASE_URL=https://api.geminigen.ai
GEMINIGEN_API_KEYS=key1,key2,key3

# File Storage
FILE_STORAGE_TYPE=local
FILE_STORAGE_PATH=./uploads
MAX_FILE_SIZE=104857600
# For S3 (optional)
# AWS_S3_BUCKET=your-bucket-name
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_REGION=us-east-1

# Queue System (Redis)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
